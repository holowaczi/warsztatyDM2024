   "source": [
    "## A przy użyciu następującej funckji możemy nasze tabele zapisywać jako plik HTML"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 160,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "savehtml (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "function savehtml(filename, data)\n",
    "    open(\"$filename.html\", \"w\") do f\n",
    "        pretty_table(f, data, backend = Val(:html), standalone = true)\n",
    "    end\n",
    "end"
   ]
  },
  {
   "cell_type": "code",
